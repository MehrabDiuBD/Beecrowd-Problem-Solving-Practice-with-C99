#include<stdio.h>
int main ()
{
    char ch1, ch2, ch3;
    scanf(" %c", &ch1);
    scanf(" %c", &ch2);
    scanf(" %c", &ch3);
    
    printf("A = %c, B = %c, C = %c\n", ch1, ch2, ch3);
    printf("A = %c, B = %c, C = %c\n", ch2, ch3, ch1);
    printf("A = %c, B = %c, C = %c\n", ch3, ch1, ch2);

    return 0;
}
